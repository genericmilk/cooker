{
    "name": "genericmilk/cooker",
    "description": "Easy frontend resource building for Laravel",
    "keywords": ["laravel", "cooker", "resources", "less", "javascript"],
    "version": "8.0.0-dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Day",
            "email": "peterday.main@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.3",
        "wikimedia/less.php": "^4.1.1",
        "scssphp/scssphp": "^1.2",
        "guzzlehttp/guzzle": "^7.5.0",
        "tedivm/jshrink": "^1.6",
        "laravel/prompts": "^0.1.15"
    },
    "autoload": {
        "psr-4": {
            "Genericmilk\\Cooker\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        },
        "laravel": {
            "providers": [
                "Genericmilk\\Cooker\\ServiceProvider"
            ]
        }
    },
    "require-dev": {
        "laravel/framework": "^10.0.0"
    }
}
