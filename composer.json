{
    "name": "genericmilk/cooker",
    "description": "Plugin to the Cooker Engine! Easy site building by Genericmilk",
    "keywords": ["laravel", "cooker", "resources", "less", "javascript"],
    "version": "6.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Day",
            "email": "peterday.main@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "wikimedia/less.php": "^3.1.0",
        "scssphp/scssphp": "^1.2",
        "nunomaduro/laravel-desktop-notifier": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "Genericmilk\\Cooker\\": "src/"
        },
        "files": [
			"src/helpers.php"
		]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        },
        "laravel": {
            "providers": [
                "Genericmilk\\Cooker\\ServiceProvider"
            ]
        }
    },
    "require-dev": {
        "laravel/framework": "^8.0.0",
        "genericmilk/telephone": "^1.5.0"
    }
}
