{
    "name": "genericmilk/cooker",
    "description": "Easy frontend resource building for Laravel",
    "keywords": ["laravel", "cooker", "resources", "less", "javascript"],
    "version": "8.0.0-dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Day",
            "email": "peterday.main@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.3",
        "wikimedia/less.php": "^5.1.2",
        "scssphp/scssphp": "^2.0.0",
        "guzzlehttp/guzzle": "^7.5.0",
        "laravel/prompts": "^0.3.2",
        "mck89/peast": "^1.16"
    },
    "autoload": {
        "psr-4": {
            "Genericmilk\\Cooker\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        },
        "laravel": {
            "providers": [
                "Genericmilk\\Cooker\\ServiceProvider"
            ]
        }
    },
    "require-dev": {
        "laravel/framework": "^10.48.23",
        "orchestra/testbench": "^8.5"
    }
}
